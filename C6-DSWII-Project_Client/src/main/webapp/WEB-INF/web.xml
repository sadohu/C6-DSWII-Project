<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ControladorDocente</servlet-name>
  	<display-name>ControladorDocente</display-name>
  	<description></description>
  	<servlet-class>Controller.ControladorDocente</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UsuarioController</servlet-name>
  	<display-name>UsuarioController</display-name>
  	<description></description>
  	<servlet-class>Controller.UsuarioController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControladorDocente</servlet-name>
  	<url-pattern>/ControladorDocente</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UsuarioController</servlet-name>
  	<url-pattern>/UsuarioController</url-pattern>
  </servlet-mapping>
</web-app>
